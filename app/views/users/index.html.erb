<div class="container-xxl flex-grow-1 container-p-y">
  <div class="card">
    <h5 class="card-header">Employees</h5>
    <div class="col">
          <h4 class="fw-bold"><span class="text-muted fw-light"><a href='/users/new' class="btn btn-primary mt-1 mb-2 timesheet-btn"><i class='bx bx-plus'></i><span>Add employee</span></a></h4>
        </div>
    <div class="table-responsive text-nowrap">
      <%= render 'shared/messages', obj: @user %>  
      <table class="table table-hover" id="employee-table">
        <thead>
            <tr>
            <th>Photo</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email id</th>
            <th>Gender</th>
            <th>Date of Birth</th>
            <th>Projects</th>
            <th>Jobs</th>
            <th>Status</th>
            <th>Actions</th>
            </tr>
        </thead>
      </table>
    </div>
  </div>
</div>
  
