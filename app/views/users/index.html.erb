<div class="container-xxl flex-grow-1 container-p-y">
  <h4 class="fw-bold py-3 mb-2"><span class="text-muted fw-light"><%= link_to('Add Employee', new_user_path, class: 'btn btn-primary') %></h4>

  <div class="card">
    <h5 class="card-header">Employees</h5>
    <div class="table-responsive text-nowrap">
      <%= render 'shared/messages', obj: @user %>  
      <table class="table table-hover" id="employee-table">
        <thead>
            <tr>
            <th>Photo</th>
            <th>User ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email id</th>
            <th>Gender</th>
            <th>Date of Birth</th>
            <th>Status</th>
            <th>Actions</th>
            </tr>
        </thead>
      </table>
    </div>
  </div>
</div>
  
